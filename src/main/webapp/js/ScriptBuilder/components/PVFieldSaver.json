{
  xtype: "form",
  id: "PVFieldSaverForm",
  nclass: PVFieldSaverNode,
  bodyStyle: "padding:5px;",
  labelWidth: 150,
  items: [{
    xtype: "textfield",
    name: "uniqueName",
    value: "fieldsaver",
    anchor: "100%",
    fieldLabel: "Unique Saver Name",
    allowBlank: false,
    maskRe: /[A-Za-z0-9._]+/
  }, {
    xtype: "combo",
    name: "fieldName",
    editable: false,
    forceSelection: true,
    mode: "local",
    triggerAction: "all",
    anchor: "100%",
    fieldLabel: "Field Name",
    store: ["displacement", "velocity", "position", "force"],
    value: "displacement"
  }, {
    xtype: "textfield",
    name: "fileName",
    value: "out.dat",
    anchor: "100%",
    fieldLabel: "Save File Name",
    allowBlank: false,
    maskRe: /[A-Za-z0-9._#-]+/
  }, {
    xtype: "combo",
    name: "fileFormat",
    editable: false,
    forceSelection: true,
    mode: "local",
    triggerAction: "all",
    anchor: "100%",
    fieldLabel: "File Format",
    store: ["SUM", "MAX", "RAW_SERIES", "POV", "DX"],
    value: "SUM"
  }, {
    xtype: "numberfield",
    name: "beginTS",
    value: "0",
    anchor: "100%",
    fieldLabel: "Timestep to Begin Saving",
    allowBlank: false,
    allowNegative: false,
    allowDecimals: false
  }, {
    xtype: "numberfield",
    name: "endTS",
    value: "1000",
    anchor: "100%",
    fieldLabel: "Timestep to End Saving",
    allowBlank: false,
    allowNegative: false,
    allowDecimals: false
  }, {
    xtype: "numberfield",
    name: "timeIncrement",
    value: "1",
    anchor: "100%",
    fieldLabel: "Timestep Increment",
    allowDecimals: false,
    allowNegative: false,
    allowBlank: false
  }]
}

