<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.auscope.portal.server.gridjob.GeodesyJob" table="jobs">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="seriesId" column="series_id"/>
        <!--<many-to-one name="series" column="series_id"/>-->
        <property name="reference"/>
        <property name="name"/>
        <property name="description"/>
        <property name="scriptFile"/>
        <property name="status"/>
        <property name="submitDate"/>
        <property name="outputDir"/>
        <property name="site"/>
        <property name="version"/>
        <property name="numTimesteps"/>
        <property name="numParticles"/>
        <property name="numBonds"/>
        <property name="checkpointPrefix"/>
        <property name="registered"/>
        <property name="extraJobDetails"/>
    </class>
</hibernate-mapping>

