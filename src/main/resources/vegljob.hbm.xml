<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.auscope.portal.server.vegl.VEGLJob" table="jobs">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="name"/>
        <property name="description"/>
        <property name="submitDate"/>
        <property name="ec2AMI"/>
        <property name="ec2InstanceId"/>
        <property name="ec2Endpoint"/>
        <property name="s3OutputAccessKey"/>
        <property name="s3OutputSecretKey"/>
        <property name="s3OutputBucket"/>
        <property name="s3OutputBaseKey"/>
        <property name="registeredUrl"/>
        <property name="emailAddress"/>
        <property name="status"/>
        <property name="seriesId"/>
        <property name="user"/>
        <property name="fileStorageId"/>
        <property name="paddingMinEasting"/>
        <property name="paddingMaxEasting"/>
        <property name="paddingMinNorthing"/>
        <property name="paddingMaxNorthing"/>
        <property name="selectionMinEasting"/>
        <property name="selectionMaxEasting"/>
        <property name="selectionMinNorthing"/>
        <property name="selectionMaxNorthing"/>
        <property name="mgaZone"/>
        <property name="cellX"/>
        <property name="cellY"/>
        <property name="cellZ"/>
        <property name="inversionDepth"/>
        <property name="vmSubsetFilePath"/>
        
    </class>
</hibernate-mapping>

