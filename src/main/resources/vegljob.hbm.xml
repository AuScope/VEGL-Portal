<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.auscope.portal.server.vegl.VEGLJob" table="jobs">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="name"/>
        <property name="description"/>
        <property name="emailAddress"/>
        <property name="user"/>
        <property name="submitDate"/>
        <property name="status"/>
        <property name="computeVmId"/>
        <property name="computeInstanceId"/>
        <property name="computeInstanceType"/>
        <property name="computeInstanceKey"/>
        <property name="storageEndpoint"/>
        <property name="storageBucket"/>
        <property name="storageAccessKey"/>
        <property name="storageSecretKey"/>
        <property name="registeredUrl"/>
        <property name="seriesId"/>
        <property name="vmSubsetFilePath"/>
        <property name="vmSubsetUrl"/>
        <property name="paddingMinEasting"/>
        <property name="paddingMaxEasting"/>
        <property name="paddingMinNorthing"/>
        <property name="paddingMaxNorthing"/>
        <property name="selectionMinEasting"/>
        <property name="selectionMaxEasting"/>
        <property name="selectionMinNorthing"/>
        <property name="selectionMaxNorthing"/>
        <property name="mgaZone"/>
        <property name="cellX"/>
        <property name="cellY"/>
        <property name="cellZ"/>
        <property name="inversionDepth"/>
        <property name="storageBaseKey"/>
        <property name="storageProvider"/>
    </class>
</hibernate-mapping>

