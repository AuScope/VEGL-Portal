<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.auscope.portal.server.vegl.VGLSignature" table="signatures">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="user"/>
        <property name="individualName"/>
        <property name="organisationName"/>
        <property name="positionName"/>
        <property name="telephone"/>
        <property name="facsimile"/>
        <property name="deliveryPoint"/>
        <property name="city"/>
        <property name="administrativeArea"/>
        <property name="postalCode"/>
        <property name="country"/>
        <property name="onlineContactName"/>
        <property name="onlineContactDescription"/>
        <property name="onlineContactURL"/>
        <property name="keywords"/>
        <property name="constraints"/>
    </class>
</hibernate-mapping>