<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.auscope.portal.server.gridjob">
    <class name="GeodesySeries" table="series">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="user"/>
        <property name="name"/>
        <property name="description"/>
        <!--
        <set name="jobs" lazy="false">
            <key column="series_id"/>
            <one-to-many class="VRLJob"/>
        </set>
        -->
    </class>
</hibernate-mapping>

